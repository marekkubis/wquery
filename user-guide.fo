                                    <fo:inline font-style="italic">(b,a)</fo:inline> such that <fo:inline font-style="italic">(a,b)</fo:inline> belongs to <fo:inline font-family="monospace">r</fo:inline>  
                                </fo:block></fo:table-cell></fo:table-row><fo:table-row><fo:table-cell padding-start="2pt" padding-end="2pt" padding-top="2pt" padding-bottom="2pt" border-bottom-width="0.5pt" border-bottom-style="solid" border-bottom-color="black" border-end-width="0.5pt" border-end-style="solid" border-end-color="black" display-align="center" text-align="center"><fo:block>transitive closure</fo:block></fo:table-cell><fo:table-cell padding-start="2pt" padding-end="2pt" padding-top="2pt" padding-bottom="2pt" border-bottom-width="0.5pt" border-bottom-style="solid" border-bottom-color="black" border-end-width="0.5pt" border-end-style="solid" border-end-color="black" display-align="center" text-align="center"><fo:block><fo:inline font-family="monospace">r!</fo:inline></fo:block></fo:table-cell><fo:table-cell padding-start="2pt" padding-end="2pt" padding-top="2pt" padding-bottom="2pt" border-bottom-width="0.5pt" border-bottom-style="solid" border-bottom-color="black" display-align="center" text-align="left"><fo:block>
                                    <fo:inline font-style="italic">(a,b)</fo:inline> such that <fo:inline font-style="italic">(a,b)</fo:inline> belongs to <fo:inline font-family="monospace">r</fo:inline>
